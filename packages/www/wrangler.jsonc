{
    "$schema": "node_modules/wrangler/config-schema.json",
    "name": "party",
    "compatibility_date": "2025-09-24",
    "compatibility_flags": ["nodejs_compat"],
    // "main": "@tanstack/solid-start/server-entry",
    "main": "src/worker/index.ts",
    "d1_databases": [
        {
            "binding": "DB",
            "database_name": "party",
            "database_id": "6cb78c0f-1457-4348-b3e9-d8074c529422",
        },
    ],
    "r2_buckets": [
        {
            "binding": "BUCKET",
            "bucket_name": "party",
            "preview_bucket_name": "party-preview",
        },
    ],
    "durable_objects": {
        "bindings": [
            {
                "name": "WS",
                "class_name": "GameRoom",
            },
        ],
    },
    "migrations": [
        {
            "tag": "v1",
            "new_sqlite_classes": ["GameRoom"],
        },
    ],
    "vars": {
        "MY_VAR": "Hello from Cloudflare",
    },
}
